		"@brandup/ui-helpers": "file:../brandup-ui-helpers",
		"cors": "^2.8.5",
		"express": "^4.21.2"
	},
	"devDependencies": {
		"@babel/plugin-transform-runtime": "^7.26.9",
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-typescript": "^7.26.0",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^20.17.22",
		"babel-loader": "^9.2.1",
		"core-js": "^3.41.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.11.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.6.3",
		"less": "^4.2.2",
		"less-loader": "^12.2.0",
		"less-plugin-clean-css": "^1.6.0",
		"mini-css-extract-plugin": "^2.9.2",
		"raw-loader": "^4.0.2",
		"style-loader": "^3.3.4",
		"svgo-loader": "^4.0.0",
		"terser-webpack-plugin": "^5.3.12",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.2",
		"url-loader": "^4.1.1",
		"webpack": "^5.98.0",
		"webpack-cli": "^5.1.4",
		"webpack-manifest-plugin": "^5.0.0"
	},
	"scripts": {
		"build": "npm run build:webpack",
		"build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js",
		"build:fallback": "cross-env BROWSERS_ENV=fallback npm run build:webpack",
		"build:modern": "cross-env BROWSERS_ENV=modern npm run build:webpack",
		"dev-build": "cross-env NODE_ENV=development webpack --config webpack.config.js",
		"watch": "cross-env NODE_ENV=development webpack --config webpack.config.js --watch",
		"start": "npm run build && tsc ./src/backend/server.ts && node ./src/backend/server.js --port=8316",
		"start-dev": "tsc ./src/backend/server.ts && node ./src/backend/server.js --port=8316"
	}
}
